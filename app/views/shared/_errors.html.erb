<% if target.errors.any? %>
  <div id="error_explanation">
    <div class="alert alert-danger">
      <%= t "form_contain" %> <%= pluralize target.errors.count, t("error") %>
    </div>
    <ul>
    <% target.errors.full_messages.each do |msg| %>
      <li><%= msg %></li>
    <% end %>
    </ul>
  </div>
<% end %>
